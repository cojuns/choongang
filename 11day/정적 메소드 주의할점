package sec05.exam01;

public class ClassName {
	 
	
	//인스턴스 변수 
	int field;
	void method1() {
	this.field = 10;
	field2 = 500;
	method2();
	}
	
	// 정적 필드, 메소드는 메모리에 할당 되있음
	// 정적 필드, 정적 메소드
	static int field2;
	static void method2() {}
	static void method3() {	
		
//	this.field = 10; // 컴파일 에러 
		
	ClassName cn = new ClassName();
	cn.field = 100;
	// 객체 생성 후 참조 변수로 접근가능
		
	field2 = 300;
	method2();
		
	}
	
}
