package sec02.exam09_04;
//static 메서드 호출 방식

public class TestExStatic {
	
	public void bbb() {
		System.out.println("bbb()");
	}
	public static void aaa() {
		System.out.println("aaa()");
	}
	public static void main(String[] args) {
		aaa();
		
		TestExStatic t1 = new TestExStatic();
		t1.bbb();

	}

}
