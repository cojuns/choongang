<script type="text/javascript">
var a = [1,3,10,2,8];
var search = 2;
var findId = -1;

for (var i=0; i < a.length; i++) {
	if( search == a[i]){
		findId = i;
		break;
	}

	}
document.writeln("인덱스:",findId,"값",a[findId]);


</script>

